<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="313.0" prefWidth="488.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.marufeb.fiverr.java.controllers.LoginController">
   <children>
      <Button layoutX="124.0" layoutY="228.0" mnemonicParsing="false" onAction="#login" prefHeight="27.0"
              prefWidth="240.0" text="Login"/>
      <TextField fx:id="name" layoutX="144.0" layoutY="83.0" prefWidth="200.0" promptText="email"/>
      <PasswordField fx:id="password" layoutX="144.0" layoutY="143.0" prefWidth="200.0" promptText="password"/>
      <Text layoutX="205.0" layoutY="29.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login panel">
         <font>
            <Font size="16.0"/>
         </font>
      </Text>
      <Text fill="#da8f8f" layoutX="178.0" layoutY="286.0" onMouseClicked="#register" strokeType="OUTSIDE"
            strokeWidth="0.0" text="or register yourself here!" textAlignment="CENTER" underline="true"/>
      <Text fx:id="error" fill="#fc0000" layoutX="144.0" layoutY="214.0" strokeType="OUTSIDE" strokeWidth="0.0"
            textAlignment="CENTER" wrappingWidth="200.0"/>
   </children>
</AnchorPane>
